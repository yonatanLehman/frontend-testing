  },
  "homepage": "https://github.com/create-oss/frontend-testing#readme",
  "devDependencies": {
    "chai": "^4.1.2",
    "chromedriver": "^2.36.0",
    "eslint": "^4.19.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-jsx": "0.0.2",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "express": "^4.16.3",
    "install": "^0.10.4",
    "jsdom": "^11.6.2",
    "mocha": "^5.0.4",
